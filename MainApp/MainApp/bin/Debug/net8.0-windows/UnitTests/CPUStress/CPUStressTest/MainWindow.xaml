<Window x:Class="CPUStressTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CPUStressTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="461" Width="543" WindowStyle="None" ResizeMode="NoResize"  WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="Lbl_Main" HorizontalAlignment="Left" Margin="79,58,0,0" VerticalAlignment="Top" Height="53" Width="414" FontFamily="Bahnschrift SemiBold Condensed" FontSize="28" FontWeight="Bold"/>
        <Image x:Name="Img_Init" HorizontalAlignment="Left" Height="111" Margin="220,161,0,0" VerticalAlignment="Top" Width="118" Source="/cpu.png" Visibility="Hidden"/>
        <Image  x:Name="Img_result"  HorizontalAlignment="Left" Height="121" Margin="215,161,0,0" VerticalAlignment="Top" Width="135" Source="/Errors.png" Visibility="Hidden"/>
        <Button x:Name="Btn_Try_Again" Content="Tentar Novamente" HorizontalAlignment="Center" Margin="0,369,0,0" VerticalAlignment="Top" Height="46" Width="173" FontFamily="Bahnschrift SemiBold Condensed" Click="Btn_Try_Again_Click" Visibility="Hidden"/>

    </Grid>
</Window>
